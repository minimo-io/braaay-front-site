{
	"name": "braaay-front-site",
	"private": true,
	"version": "0.8.6",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"prebuild": "tsc -p scripts/tsconfig.scripts.json && node scripts/update-grapes.js && node scripts/update-subcategories.js && node scripts/update-countries.js && node scripts/generate-robots.js && npm run process:popular-products && npm run prefetch:on-sale-products",
		"process:popular-products": "tsc -p scripts/tsconfig.scripts.json && node scripts/update-popular-products.js",
		"prefetch:on-sale-products": "tsc -p scripts/tsconfig.scripts.json && node scripts/prefetch-on-sale-products.js",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"route-tree": "node scripts/route-tree.js"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@inlang/paraglide-js": "2.0.1",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/kit": "^2.17.2",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@types/he": "^1.2.3",
		"@types/js-cookie": "^3.0.6",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.14.0",
		"postcss": "^8.5.2",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.20.4",
		"svelte-check": "^4.0.0",
		"svoast": "^3.0.3",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.0.0"
	},
	"dependencies": {
		"@fontsource-variable/open-sans": "^5.1.1",
		"@fontsource/prata": "^5.1.1",
		"@glidejs/glide": "^3.7.1",
		"@lucide/svelte": "^0.484.0",
		"@types/glidejs__glide": "^3.6.6",
		"@urql/core": "^5.1.1",
		"animate.css": "^4.1.1",
		"cpf-cnpj-validator": "^1.0.3",
		"graphql": "^16.10.0",
		"he": "^1.2.0",
		"imask": "^7.6.1",
		"js-cookie": "^3.0.5",
		"mercadopago": "^2.7.0",
		"pino": "^9.6.0",
		"pino-pretty": "^13.0.0"
	}
}
